задание 47 язык C++
#include <iostream>
#include <set>  // Для хранения уникальных значений

using namespace std;

int main() {
    int N;  // Размер массива
    cout << "Введите размер массива N: ";
    cin >> N;

    int arr[N];  // Объявление массива размера N

    // Ввод элементов массива
    cout << "Введите элементы массива: ";
    for (int i = 0; i < N; i++) {
        cin >> arr[i];
    }

    set<int> uniqueElements;  // Множество для хранения уникальных элементов
    int i = 0;                // Счетчик для while

    // Цикл с предусловием
    while (i < N) {
        uniqueElements.insert(arr[i]);  // Добавляем элемент в множество
        i++;  // Переход к следующему элементу
    }

    // Вывод количества различных элементов
    cout << "Количество различных элементов: " << uniqueElements.size() << endl;

    return 0;
}
