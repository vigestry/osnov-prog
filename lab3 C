#include <stdio.h>

int main() {
    int last_digit;
    
    // Вводим последнюю цифру числа
    printf("Введите последнюю цифру числа: ");
    scanf("%d", &last_digit);
    
    switch(last_digit) {
        case 0:
            printf("Последняя цифра квадрата: 0\n");
            break;
        case 1:
            printf("Последняя цифра квадрата: 1\n");
            break;
        case 2:
            printf("Последняя цифра квадрата: 4\n");
            break;
        case 3:
            printf("Последняя цифра квадрата: 9\n");
            break;
        case 4:
            printf("Последняя цифра квадрата: 6\n");
            break;
        case 5:
            printf("Последняя цифра квадрата: 5\n");
            break;
        case 6:
            printf("Последняя цифра квадрата: 6\n");
            break;
        case 7:
            printf("Последняя цифра квадрата: 9\n");
            break;
        case 8:
            printf("Последняя цифра квадрата: 4\n");
            break;
        case 9:
            printf("Последняя цифра квадрата: 1\n");
            break;
        default:
            printf("Ошибка ввода.\n");
            break;
    }
    
    return 0;
}
