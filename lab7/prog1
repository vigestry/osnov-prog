задание 26 язык C
#include <stdio.h>

int main() {
    int N, i, ratio;

    // Ввод размера массива
    printf("Введите размер массива (N): ");
    scanf("%d", &N);

    // Проверка, что массив содержит как минимум 2 элемента
    if (N < 2) {
        printf("0\n"); // Геометрическая прогрессия невозможна
        return 0;
    }

    int arr[N];

    // Ввод элементов массива
    printf("Введите %d ненулевых целых чисел:\n", N);
    for (i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    // Вычисление общего знаменателя на основе первых двух элементов
    if (arr[1] % arr[0] != 0) {
        printf("0\n"); // Знаменатель не целый, прогрессии нет
        return 0;
    }
    ratio = arr[1] / arr[0];

    // Проверка всех пар элементов на соответствие геометрической прогрессии
    for (i = 1; i < N - 1; i++) {
        if (arr[i + 1] % arr[i] != 0 || arr[i + 1] / arr[i] != ratio) {
            printf("0\n"); // Условие прогрессии нарушено
            return 0;
        }
    }

    // Если все проверки пройдены, выводим общий знаменатель
    printf("Знаменатель прогрессии: %d\n", ratio);

    return 0;
}
